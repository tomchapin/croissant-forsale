<div class="page-header">
  <h1>All Items for sale</h1>
</div>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @items.each do |item| %>
  <tr>
    <td>
      <%= render partial: "sentence#{item.sentence_id || 1}", locals: { item: item } %>
    </td>
  </tr>
    <% end %>
  </tbody>
</table>
